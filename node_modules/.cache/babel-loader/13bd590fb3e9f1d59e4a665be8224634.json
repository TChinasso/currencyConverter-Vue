AA,cAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA;AAJA;AAzBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n   <div class=\"column is-half is is-offset-one-quarter is-black\">\n     <div>\n     <input class=\"input is-half metade\" v-model=\"moneyIn\" >\n     <select class=\"select\" name=\"moneyIn\" id=\"moneyIn\" v-model=\"moneyInOption\">\n     <option v-for=\"(value, name, index) in siglas.rates\" :key=\"index\" :value=\"value\" :moneyInOption=\"value\">\n       {{name}}\n     </option>\n   </select>\n   </div>\n   <input class=\"metade input is-half is-is-inline \"  v-model=\"moneyOut\" @click=\"calculateMoney\">\n   <select class=\"select is-inline\" name=\"moneyOut\" id=\"moneyOut\" v-model=\"moneyOutOption\">\n     <option v-for=\"(value, name, index) in siglas.rates\" :value=\"value\" :key=\"index\" >\n       {{name}}\n     </option>\n   </select>\n  </div>\n   </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  \n  name: 'App',\n  components: {\n  },\n  data() {\n    return {\n      moneyIn: '',\n      moneyOut: '',\n      moneyInOption: '',\n      moneyOutOption: '',\n      EUR: \"\",\n      siglas: {}\n    }\n  },\n  created: function(){\n    axios.get(\"https://api.exchangeratesapi.io/latest\").then(response => {\n      this.EUR = response.data\n      this.siglas = response.data\n      this.moneyInOption = response.data.rates.BRL\n      this.moneyOutOption = response.data.rates.USD\n      console.log(this.siglas.rates.BRL)\n    })\n  },\n  \n  methods: {\n    calculateMoney: function(){\n      this.moneyOut = (Number(this.moneyIn) / this.moneyInOption)  / (1 / this.moneyOutOption)\n      console.log(this.moneyOut)\n    }\n  },\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n.metade{\n  width: 10px;\n}\n</style>\n"],"sourceRoot":"src"}]}